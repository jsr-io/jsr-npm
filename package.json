{
  "name": "jsr",
  "version": "0.13.4",
  "description": "jsr.io package manager for npm, yarn, pnpm and bun",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsr-io/jsr-npm.git"
  },
  "bin": {
    "jsr": "dist/bin.js"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "node --test --experimental-strip-types test/**.test.ts",
    "test:coverage": "node --test --experimental-strip-types --experimental-test-coverage test/**.test.ts",
    "cli": "node --experimental-strip-types src/bin.ts",
    "build": "rimraf dist dist-esm && tsc && tsc -p tsconfig.esm.json",
    "prepack": "node --run build"
  },
  "keywords": [
    "install",
    "modules",
    "package manager",
    "package.json",
    "dependencies",
    "npm"
  ],
  "homepage": "https://jsr.io",
  "author": "",
  "license": "MIT",
  "files": [
    "dist/",
    "dist-esm/"
  ],
  "devDependencies": {
    "@types/node": "^22.9.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "node-stream-zip": "^1.15.0",
    "semiver": "^1.1.0"
  }
}
